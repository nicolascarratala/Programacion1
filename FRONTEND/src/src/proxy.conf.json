{
  "/apiusers": {
      "target": "http://127.0.0.1:7000",
      "secure": false,
      "logLevel": "debug",
      "pathRewrite": {
          "^/apiusers": "/users"
      }
},
"/apiuser/*": {
      "target": "http://127.0.0.1:7000",
      "secure": false,
      "logLevel": "debug",
      "pathRewrite": {
          "^/apiuser/": "/user/"
      }
},
"/apilogin": {
  "target": "http://127.0.0.1:7000",
  "secure": false,
  "logLevel": "debug",
  "pathRewrite": {
      "^/apilogin": "/auth/login"
  }
},
"/apipoems": {
    "target": "http://127.0.0.1:7000",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
        "^/apipoems": "/poems"
    }
  },
  "/apipoem/*": {
    "target": "http://127.0.0.1:7000",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
        "^/apipoem/": "/poem/"
    }
},
}
