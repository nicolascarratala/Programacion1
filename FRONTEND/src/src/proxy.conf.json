{
  "/users": {
      "target": "http://127.0.0.1:7000",
      "secure": false,
      "logLevel": "debug",
      "pathRewrite": {
          "^/users": "/users"
      }
},
"/user/*": {
      "target": "http://127.0.0.1:7000",
      "secure": false,
      "logLevel": "debug",
      "pathRewrite": {
          "^/user/": "/user/"
      }
},
"/loginauth": {
  "target": "http://127.0.0.1:7000",
  "secure": false,
  "logLevel": "debug",
  "pathRewrite": {
      "^/loginauth": "/auth/login"
  }
},
"/poems": {
    "target": "http://127.0.0.1:7000",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
        "^/poems": "/poems"
    }
  }
}
